{
  "ModelConfigurations": [
    {
      "Name": "AzureProd",
      "Provider": "Azure",
      "Uri": "https://your-resource.cognitiveservices.azure.com/",
      "Model": "gpt-4.1-mini",
      "ApiKeyAlias": "AZURE_KEY"
    },
    {
      "Name": "OpenAIProd",
      "Provider": "OpenAI",
      "Uri": "https://api.openai.com/v1",
      "Model": "gpt-4.1-mini",
